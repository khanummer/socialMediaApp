<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users Index</title>
    <!-- Importing Css -->
    <link rel="stylesheet" type="text/css" href="/../css/styles.css">
</head>
<body>
    <h1>Users Index</h1>
    <!-- NavBar -->
    <div>
        <div><a href="/">Home</a></div>
        <div><a href="/users/new">Log in / Register</a></div>
        <div><a href="/users/index">Users Index</a></div>
        <div><a href="/users/settings">Settings</a></div>
    </div>

    <div>
        <ul>
            <% for (let i = 0; i < users.length; i++) { %>
            <li>
                <!-- <div> -->
                    ID <%=users[i]._id %>
                    Username <%=users[i].username %>
                    Bio <%=users[i].bio %>
                <!-- </div> -->
                <% } %>
            </li>

        </ul>
    </div>
</body>
</html>